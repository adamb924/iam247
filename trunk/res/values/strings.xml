    <!-- Regular expressions for parsing messages -->
    <string name="re_turnoncallaround">(?:turn\\s*on\\s*call\\s*around|turn\\s*call\\s*around\\s*on)</string>
    <string name="re_turnoffcallaround">(?:turn\\s*off\\s*call\\s*around|turn\\s*call\\s*around\\s*off)</string>
    <string name="re_callaround_ok">(?:all in|777)</string>
    <string name="re_checkin_back">back</string>
    <string name="re_distress">help|666</string>
    <string name="re_thisis">this\\s*is\\s*(.*)</string>
    <string name="re_startcheckin">going\\s*to\\s*(.*)\\s*(?:back|check.?in)\\s*(?:by|at)\\s*(.*)</string>
    <string name="re_startcheckin_nocheckin">going\\s*to\\s*([^\\s]*)\\s*back\\s*(?:by|at)\\s*(.*)\\s*no\\s*checkin</string>
    <string name="re_permission">permission|travel</string>
    <string name="re_time">^(\\d{1,2})?\\s?[\\p{Punct}]?\\s?(\\d{2}|)(a|p)?m?$</string>
    <string name="re_undocallaround">(?:undo|cancel)\\s*call\\s*around</string>
    <string name="re_turnoffreminders">(?:turn\\s*off\\s*reminders|turn\\s*reminders\\s*off)</string>
    <string name="re_turnonreminders">(?:turn\\s*on\\s*reminders|turn\\s*reminders\\s*on)</string>
    <string name="re_report">report</string>

